
<!DOCTYPE html>
<html lang="zh-cn" class="app">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="renderer" content="webkit" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" href="~/lib/jPlayer/jplayer.flat.css" type="text/css" />
    <link rel="stylesheet" href="~/css/bootstrap.css" type="text/css" />
    <link rel="stylesheet" href="~/css/animate.css" type="text/css" />
    <link rel="stylesheet" href="~/css/font-awesome.min.css" type="text/css" />
    <link rel="stylesheet" href="~/css/simple-line-icons.css" type="text/css" />
    <link rel="stylesheet" href="~/css/font.css" type="text/css" />
    <link rel="stylesheet" href="~/css/app.css" type="text/css" />
    <!--[if lt IE 9]>
      <script src="~/lib/ie/html5shiv.js"></script>
      <script src="~/lib/ie/respond.min.js"></script>
      <script src="~/lib/ie/excanvas.js"></script>
    <![endif]-->
    <script src="~/lib/jquery.min.js"></script>
    <!-- Bootstrap -->
    <script src="~/lib/bootstrap.js"></script>
    <!-- flavr -->
    <link rel="stylesheet" href="~/lib/flavr/css/flavr.css" type="text/css" />
    <script src="~/lib/flavr/js/flavr.min.js"></script>
    <!-- App -->
    <script src="~/lib/app.js"></script>
    <script src="~/lib/slimscroll/jquery.slimscroll.min.js"></script>
    <script src="~/lib/app.plugin.js"></script>
    <!-- tab栏 -->
    <link href="~/lib/jquery.scrollbar/jquery.scrollbar.min.css" rel="stylesheet" />
    <script src="~/lib/jquery.scrollbar/jquery.scrollbar.min.js"></script>
    <link href="~/lib/nth-tabs/css/nth.tabs.min.css" rel="stylesheet" />
    <script src="~/lib/nth-tabs/js/nth.tabs.min.js"></script>
    <!-- Core -->
    <script src="~/lib/jquery.core.js"></script>
    @RenderSection("Styles", required: false)
</head>
<body class="@ViewData["BodyClass"]">
    @RenderBody()
    @RenderSection("Scripts", required: false)
</body>
</html>

